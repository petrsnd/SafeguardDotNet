dotnet_naming_rule.private_static_readonly_fields_should_be_pascal_case.style = pascal_case
dotnet_naming_rule.private_static_readonly_fields_should_be_pascal_case.symbols = private_static_readonly_fields
dotnet_naming_rule.public_static_readonly_fields_should_be_pascal_case.severity = error
dotnet_naming_rule.public_static_readonly_fields_should_be_pascal_case.style = pascal_case
dotnet_naming_rule.public_static_readonly_fields_should_be_pascal_case.symbols = public_static_readonly_fields

